<?xml version="1.0" ?>
<params name="quickrace">
	<section name="Header">
		<attstr name="name" val="quickrace"/>
		<attstr name="description" val="quickrace"/>
		<attnum name="priority" val="10"/>
	</section>
	<section name="Tracks">
		<attnum name="maximum number" val="20"/>
        %%TRACK%%
	</section>
	<section name="Races">
		<section name="1">
			<attstr name="name" val="quickrace"/>
		</section>
	</section>
	<section name="quickrace">
		<attnum name="distance" unit="km" val="0"/>
		<attstr name="type" val="race"/>
		<attstr name="starting order" val="random"/>
		<attstr name="restart" val="false"/>
		<attnum name="maximum drivers" val="40"/>
		<attstr name="must complete" val="yes"/>
		<attnum name="laps" val="2"/>
		<section name="Starting Grid">
			<attnum name="rows" val="2.000000"/>
			<attnum name="distance to start" val="25.000000"/>
			<attnum name="distance between columns" val="30.000000"/>
			<attnum name="offset within a column" val="10.000000"/>
			<attnum name="initial speed" val="0.000000"/>
			<attnum name="initial height" val="0.200000"/>
		</section>
	</section>
	<section name="Drivers">
		<attnum name="maximum number" val="50"/>
		<attstr name="focused module" val="scr_server"/>
		<attnum name="focused idx" val="4"/>
		<section name="1">
			<attnum name="idx" val="3"/>
			<attstr name="module" val="tita"/>
		</section>
		<section name="2">
			<attnum name="idx" val="3"/>
			<attstr name="module" val="berniw"/>
		</section>
		<section name="3">
			<attnum name="idx" val="3"/>
			<attstr name="module" val="olethros"/>
		</section>
        <section name="4">
            <attnum name="idx" val="1"/>
            <attstr name="module" val="scr_server"/>
        </section>
		<section name="5">
			<attnum name="idx" val="3"/>
			<attstr name="module" val="lliaw"/>
		</section>
		<section name="6">
			<attnum name="idx" val="3"/>
			<attstr name="module" val="bt"/>
		</section>

	</section>
</params>
